local MenuGroup = Tabs.UI:AddLeftGroupbox("菜單設置", "settings");
MenuGroup:AddToggle("CustomCursor", {Text="啟用自定義光標",Default=true,Callback=function(v)
	Library.ShowCustomCursor = v;
end});
MenuGroup:AddDropdown("NotifySide", {Values={"Left","Right"},Default="Right",Text="通知顯示位置",Callback=function(v)
	Library:SetNotifySide(v);
end});
MenuGroup:AddButton("卸載腳本", function()
	Library:Unload();
end);
ThemeManager:SetLibrary(Library);
SaveManager:SetLibrary(Library);
SaveManager:IgnoreThemeSettings();
ThemeManager:SetFolder("ChenMoScript");
SaveManager:SetFolder("ChenMoScript/Game");
SaveManager:BuildConfigSection(Tabs.UI);
ThemeManager:ApplyToTab(Tabs.UI);
SaveManager:LoadAutoloadConfig();
print("陳某腳本完整版載入完成 - 所有 Tab 已補齊");
Library:Notify("完整版腳本已載入（含所有 Tab 功能）", 8);