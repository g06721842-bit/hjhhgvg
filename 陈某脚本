task.spawn(function()
	local FlatIdent_8D327 = 0;
	while true do
		if (FlatIdent_8D327 == 0) then
			task.wait(2.5);
			if (UserName == AuthorName) then
				local FlatIdent_24A02 = 0;
				while true do
					if (FlatIdent_24A02 == 0) then
						Library:Notify("歡迎陳某腳本作者本人！", 10);
						pcall(function()
							if (TextChatService.ChatVersion == Enum.ChatVersion.TextChatService) then
								TextChatService.TextChannels.RBXGeneral:SendAsync("陳某腳本作者已進入遊戲");
							end
						end);
						break;
					end
				end
			elseif (UserName == PartnerName) then
				local FlatIdent_89562 = 0;
				while true do
					if (FlatIdent_89562 == 0) then
						Library:Notify("歡迎合作人 faggot", 10);
						pcall(function()
							if (TextChatService.ChatVersion == Enum.ChatVersion.TextChatService) then
								TextChatService.TextChannels.RBXGeneral:SendAsync("合作人 faggot 已加入遊戲");
							end
						end);
						break;
					end
				end
			else
				Library:Notify("腳本加載成功！歡迎使用陈某腳本", 5);
			end
			break;
		end
	end
end);
local Tabs = {Main=Window:AddTab("通用內容", "user"),Gun=Window:AddTab("陈某枪战功能", "crosshair"),BladeBall=Window:AddTab("刀刃球", "swords"),Seam=Window:AddTab("縫合腳本", "puzzle"),Battlefield=Window:AddTab("戰場腳本", "sword"),UI=Window:AddTab("UI 設置", "settings"),Lc=Window:AddTab("lc", "user")};
local nightVisionEnabled = false;
local nightVisionConnection;
local function EnableNightVision()
	local FlatIdent_10DED = 0;
	while true do
		if (2 == FlatIdent_10DED) then
			Lighting.ColorShift_Top = Color3.new(1, 1, 1);
			nightVisionConnection = RunService.RenderStepped:Connect(function()
				if nightVisionEnabled then
					for _, obj in pairs(Workspace:GetDescendants()) do
						if (obj:IsA("BasePart") and not obj.Parent:FindFirstChild("Humanoid")) then
							local FlatIdent_7126A = 0;
							while true do
								if (FlatIdent_7126A == 0) then
									obj.Material = Enum.Material.Neon;
									obj.Color = Color3.new(0, 1, 0);
									break;
								end
							end
						end
					end
				end
			end);
			break;
		end
		if (FlatIdent_10DED == 1) then
			Lighting.Ambient = Color3.new(1, 1, 1);
			Lighting.ColorShift_Bottom = Color3.new(1, 1, 1);
			FlatIdent_10DED = 2;
		end
		if (FlatIdent_10DED == 0) then
			if nightVisionConnection then
				nightVisionConnection:Disconnect();
			end
			nightVisionEnabled = true;
			FlatIdent_10DED = 1;
		end
	end
end
local function DisableNightVision()
	local FlatIdent_145D2 = 0;
	while true do
		if (FlatIdent_145D2 == 0) then
			nightVisionEnabled = false;
			if nightVisionConnection then
				local FlatIdent_12703 = 0;
				while true do
					if (FlatIdent_12703 == 0) then
						nightVisionConnection:Disconnect();
						nightVisionConnection = nil;
						break;
					end
				end
			end
			FlatIdent_145D2 = 1;
		end
		if (FlatIdent_145D2 == 1) then
			Lighting.Ambient = Color3.new(0, 0, 0);
			Lighting.ColorShift_Bottom = Color3.new(0, 0, 0);
			FlatIdent_145D2 = 2;
		end
		if (2 == FlatIdent_145D2) then
			Lighting.ColorShift_Top = Color3.new(0, 0, 0);
			for _, obj in pairs(Workspace:GetDescendants()) do
				if obj:IsA("BasePart") then
					obj.Material = Enum.Material.Plastic;
				end
			end
			break;
		end
	end
end
local PingSection = Tabs.Main:AddLeftGroupbox("網絡狀態", "activity");
local TimeLabel = PingSection:AddLabel("時間: 載入中...", true);
local PingLabel = PingSection:AddLabel("Ping: 檢測中...", true);
local FPSLabel = PingSection:AddLabel("FPS: 檢測中...", true);
local NetLabel = PingSection:AddLabel("網絡: 檢測中...", true);
task.spawn(function()
	local FlatIdent_656E9 = 0;
	local lastTime;
	local frameCount;
	while true do
		if (FlatIdent_656E9 == 0) then
			lastTime, frameCount = tick(), 0;
			while task.wait(0.5) do
				local FlatIdent_2BD95 = 0;
				local now;
				while true do
					if (FlatIdent_2BD95 == 1) then
						if ((now - lastTime) >= 1) then
							local FlatIdent_1D164 = 0;
							local fps;
							local ping;
							local status;
							while true do
								if (FlatIdent_1D164 == 2) then
									if ((ping < 80) and (fps > 50)) then
										status = "極佳";
									elseif ((ping < 150) and (fps > 30)) then
										status = "良好";
									end
									TimeLabel:SetText("時間: " .. os.date("%H:%M"));
									PingLabel:SetText("Ping: " .. ping .. "ms");
									FlatIdent_1D164 = 3;
								end
								if (FlatIdent_1D164 == 1) then
									ping = math.floor(game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue() or 60);
									if (ping <= 0) then
										ping = math.random(30, 120);
									end
									status = "一般";
									FlatIdent_1D164 = 2;
								end
								if (FlatIdent_1D164 == 3) then
									FPSLabel:SetText("FPS: " .. fps);
									NetLabel:SetText("網絡: " .. status);
									break;
								end
								if (FlatIdent_1D164 == 0) then
									fps = frameCount;
									frameCount = 0;
									lastTime = now;
									FlatIdent_1D164 = 1;
								end
							end
						end
						break;
					end
					if (FlatIdent_2BD95 == 0) then
						frameCount += 1
						now = tick();
						FlatIdent_2BD95 = 1;
					end
				end
			end
			break;
		end
	end
end);
local CharSection = Tabs.Main:AddLeftGroupbox("角色控制", "user");
CharSection:AddToggle("InfJump", {Text="無限跳躍",Default=false,Callback=function(v)
	if v then
		getgenv().InfJumpConn = UserInputService.JumpRequest:Connect(function()
			local FlatIdent_60EA1 = 0;
			local hum;
			while true do
				if (FlatIdent_60EA1 == 0) then
					hum = LP.Character and LP.Character:FindFirstChildOfClass("Humanoid");
					if hum then
						hum:ChangeState(Enum.HumanoidStateType.Jumping);
					end
					break;
				end
			end
		end);
	elseif getgenv().InfJumpConn then
		getgenv().InfJumpConn:Disconnect();
	end
end});
CharSection:AddSlider("WalkSpeed", {Text="移動速度",Default=16,Min=16,Max=400,Rounding=0,Callback=function(v)
