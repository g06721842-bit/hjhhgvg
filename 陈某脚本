local AimbotSection = Tabs.Gun:AddLeftGroupbox("自瞄設置", "target");
local aimbotEnabled = false;
local aimbotConn;
local aimPart = "Head";
local teamCheck = true;
local wallCheck = true;
local fovRadius = 150;
local smoothing = 0.2;
local alwaysAimMobile = true;
local fovCircle = Drawing.new("Circle");
fovCircle.Visible = false;
fovCircle.Thickness = 2;
fovCircle.Color = Color3.fromRGB(255, 0, 0);
fovCircle.Radius = fovRadius;
fovCircle.NumSides = 100;
fovCircle.Filled = false;
local function isVisible(part)
	if not wallCheck then
		return true;
	end
	local cam = workspace.CurrentCamera;
	local origin = cam.CFrame.Position;
	local dir = part.Position - origin;
	local params = RaycastParams.new();
	params.FilterDescendantsInstances = {LP.Character};
	params.FilterType = Enum.RaycastFilterType.Blacklist;
	local res = workspace:Raycast(origin, dir, params);
	return not res or res.Instance:IsDescendantOf(part.Parent);
end
local function getClosest()
	local cam = workspace.CurrentCamera;
	local mouse = UserInputService:GetMouseLocation();
	local closest, dist = nil, fovRadius;
	for _, plr in Players:GetPlayers() do
		if ((plr ~= LP) and plr.Character) then
			local FlatIdent_20FE3 = 0;
			local hum;
			while true do
				if (FlatIdent_20FE3 == 0) then
					hum = plr.Character:FindFirstChildOfClass("Humanoid");
					if (hum and (hum.Health > 0) and plr.Character:FindFirstChild(aimPart)) then
						local FlatIdent_578E3 = 0;
						local part;
						local pos;
						local onScreen;
						while true do
							if (FlatIdent_578E3 == 0) then
								if (teamCheck and (plr.Team == LP.Team)) then
									continue;
								end
								part = plr.Character[aimPart];
								FlatIdent_578E3 = 1;
							end
							if (FlatIdent_578E3 == 1) then
								pos, onScreen = cam:WorldToViewportPoint(part.Position);
								if onScreen then
									local FlatIdent_40070 = 0;
									local d;
									while true do
										if (FlatIdent_40070 == 0) then
											d = (Vector2.new(pos.X, pos.Y) - mouse).Magnitude;
											if ((d < dist) and isVisible(part)) then
												dist = d;
												closest = part;
											end
											break;
										end
									end
								end
								break;
							end
						end
					end
					break;
				end
			end
		end
	end
	return closest;
end
AimbotSection:AddToggle("AimbotEnable", {Text="啟用自瞄 + FOV圈",Default=false,Callback=function(v)
	local FlatIdent_86900 = 0;
	while true do
		if (1 == FlatIdent_86900) then
			if v then
				aimbotConn = RunService.RenderStepped:Connect(function()
					local FlatIdent_91B54 = 0;
					local shouldAim;
					local target;
					while true do
						if (FlatIdent_91B54 == 0) then
							fovCircle.Position = UserInputService:GetMouseLocation();
							shouldAim = UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton2) or (UserInputService.TouchEnabled and alwaysAimMobile);
							FlatIdent_91B54 = 1;
						end
						if (FlatIdent_91B54 == 2) then
							if target then
								local cam = workspace.CurrentCamera;
								local cf = cam.CFrame;
								local dir = (target.Position - cf.Position).Unit;
								cam.CFrame = cf:Lerp(CFrame.new(cf.Position, cf.Position + (dir * 100)), smoothing);
							end
							break;
						end
						if (FlatIdent_91B54 == 1) then
							if not shouldAim then
								return;
							end
							target = getClosest();
							FlatIdent_91B54 = 2;
						end
					end
				end);
			elseif aimbotConn then
				local FlatIdent_331F0 = 0;
				while true do
					if (FlatIdent_331F0 == 0) then
						aimbotConn:Disconnect();
						aimbotConn = nil;
						break;
					end
				end
			end
			break;
		end
		if (FlatIdent_86900 == 0) then
			aimbotEnabled = v;
			fovCircle.Visible = v;
			FlatIdent_86900 = 1;
		end
	end
end});
AimbotSection:AddSlider("FOVRadius", {Text="FOV 半徑",Default=150,Min=50,Max=400,Rounding=0,Callback=function(v)
	local FlatIdent_53124 = 0;
	while true do
		if (FlatIdent_53124 == 0) then
			fovRadius = v;
			fovCircle.Radius = v;
