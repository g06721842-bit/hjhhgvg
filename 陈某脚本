							end
						elseif (((ctrl.l + ctrl.r) == 0) and ((ctrl.f + ctrl.b) == 0) and (speed ~= 0)) then
							bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f + lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l + lastctrl.r, (lastctrl.f + lastctrl.b) * 0.2, 0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p)) * speed;
						else
							bv.velocity = Vector3.new(0, 0, 0);
						end
						bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad(((ctrl.f + ctrl.b) * 50 * speed) / maxspeed), 0, 0);
						break;
					end
					if (FlatIdent_61F8E == 0) then
						game:GetService("RunService").RenderStepped:Wait();
						if (((ctrl.l + ctrl.r) ~= 0) or ((ctrl.f + ctrl.b) ~= 0)) then
							local FlatIdent_7993F = 0;
							while true do
								if (FlatIdent_7993F == 0) then
									speed = speed + 0.5 + (speed / maxspeed);
									if (speed > maxspeed) then
										speed = maxspeed;
									end
									break;
								end
							end
						elseif (not (((ctrl.l + ctrl.r) ~= 0) or ((ctrl.f + ctrl.b) ~= 0)) and (speed ~= 0)) then
							local FlatIdent_6A83E = 0;
							while true do
								if (FlatIdent_6A83E == 0) then
									speed = speed - 1;
									if (speed < 0) then
										speed = 0;
									end
									break;
								end
							end
						end
						FlatIdent_61F8E = 1;
					end
				end
			end
			ctrl = {f=0,b=0,l=0,r=0};
			lastctrl = {f=0,b=0,l=0,r=0};
			speed = 0;
			bg:Destroy();
			bv:Destroy();
			plr.Character.Humanoid.PlatformStand = false;
			game.Players.LocalPlayer.Character.Animate.Disabled = false;
			tpwalking = false;
		else
			local plr = game.Players.LocalPlayer;
			local UpperTorso = plr.Character.UpperTorso;
			local flying = true;
			local deb = true;
			local ctrl = {f=0,b=0,l=0,r=0};
			local lastctrl = {f=0,b=0,l=0,r=0};
			local maxspeed = 50;
			local speed = 0;
			local bg = Instance.new("BodyGyro", UpperTorso);
			bg.P = 90000;
			bg.maxTorque = Vector3.new(8999999488, 8999999488, 8999999488);
			bg.cframe = UpperTorso.CFrame;
			local bv = Instance.new("BodyVelocity", UpperTorso);
			bv.velocity = Vector3.new(0, 0.1, 0);
			bv.maxForce = Vector3.new(8999999488, 8999999488, 8999999488);
			if (nowe == true) then
				plr.Character.Humanoid.PlatformStand = true;
			end
			while (nowe == true) or (game:GetService("Players").LocalPlayer.Character.Humanoid.Health == 0) do
				local FlatIdent_2D2B8 = 0;
				while true do
					if (FlatIdent_2D2B8 == 1) then
						if (((ctrl.l + ctrl.r) ~= 0) or ((ctrl.f + ctrl.b) ~= 0)) then
							local FlatIdent_29B3D = 0;
							while true do
								if (FlatIdent_29B3D == 0) then
									bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f + ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l + ctrl.r, (ctrl.f + ctrl.b) * 0.2, 0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p)) * speed;
									lastctrl = {f=ctrl.f,b=ctrl.b,l=ctrl.l,r=ctrl.r};
									break;
								end
							end
						elseif (((ctrl.l + ctrl.r) == 0) and ((ctrl.f + ctrl.b) == 0) and (speed ~= 0)) then
							bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f + lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l + lastctrl.r, (lastctrl.f + lastctrl.b) * 0.2, 0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p)) * speed;
						else
							bv.velocity = Vector3.new(0, 0, 0);
						end
						bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad(((ctrl.f + ctrl.b) * 50 * speed) / maxspeed), 0, 0);
						break;
					end
					if (FlatIdent_2D2B8 == 0) then
						wait();
						if (((ctrl.l + ctrl.r) ~= 0) or ((ctrl.f + ctrl.b) ~= 0)) then
							local FlatIdent_3416F = 0;
							while true do
								if (FlatIdent_3416F == 0) then
									speed = speed + 0.5 + (speed / maxspeed);
									if (speed > maxspeed) then
										speed = maxspeed;
									end
									break;
								end
							end
						elseif (not (((ctrl.l + ctrl.r) ~= 0) or ((ctrl.f + ctrl.b) ~= 0)) and (speed ~= 0)) then
							local FlatIdent_15354 = 0;
							while true do
								if (FlatIdent_15354 == 0) then
									speed = speed - 1;
									if (speed < 0) then
										speed = 0;
									end
									break;
								end
							end
						end
						FlatIdent_2D2B8 = 1;
					end
				end
			end
			ctrl = {f=0,b=0,l=0,r=0};
			lastctrl = {f=0,b=0,l=0,r=0};
			speed = 0;
			bg:Destroy();
			bv:Destroy();
			plr.Character.Humanoid.PlatformStand = false;
			game.Players.LocalPlayer.Character.Animate.Disabled = false;
			tpwalking = false;
		end
	end);
	local tis;
	up.MouseButton1Down:connect(function()
		tis = up.MouseEnter:connect(function()
			while tis do
				local FlatIdent_3EEE1 = 0;
				while true do
					if (FlatIdent_3EEE1 == 0) then
						wait();
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 1, 0);
						break;
					end
