		getgenv().GodConn = RunService.Heartbeat:Connect(function()
			local FlatIdent_6066D = 0;
			local hum;
			while true do
				if (FlatIdent_6066D == 0) then
					hum = LP.Character and LP.Character:FindFirstChildOfClass("Humanoid");
