-- HD Admin å®‰å…¨è°ƒç”¨
local function SafeHD(cmd)
    if Debounce.HD then return end  -- å…¨å±€HDé˜²æŠ–
    Debounce.HD = true
    SafeWait(0.5)  -- HDå‘½ä»¤å»¶æ—¶0.5s
    local RequestCommand = game:GetService("ReplicatedStorage"):WaitForChild("HDAdminHDClient").Signals.RequestCommandSilent
    AddDelay(function() RequestCommand:InvokeServer(cmd) end)
    task.wait(1.2)  -- å†·å´1.2s
    Debounce.HD = false
end

-- è´´å›¾è½°ç‚¸ (ä¼˜åŒ–å¾ªç¯ + å»¶æ—¶)
DecalBtn.MouseButton1Click:Connect(function()
    if Debounce.Decal then return end
    Debounce.Decal = true
    
    local player = game.Players.LocalPlayer
    local char = player.Character
    local tool
    for i,v in player:GetDescendants() do if v.Name == "SyncAPI" then tool = v.Parent end end
    for i,v in game.ReplicatedStorage:GetDescendants() do if v.Name == "SyncAPI" then tool = v.Parent end end
    local remote = tool.SyncAPI.ServerEndpoint
    function _(args) AddDelay(function() remote:InvokeServer(unpack(args)) end) end
    
    function SetLocked(part,boolean) 
        local args = {[1] = "SetLocked",[2] = {[1] = part},[3] = boolean} 
        _(args) 
    end
    function SpawnDecal(part,side) 
        local args = {[1] = "CreateTextures",[2] = {[1] = {["Part"] = part,["Face"] = side,["TextureType"] = "Decal"}}} 
        _(args) 
    end
    function AddDecal(part,asset,side) 
        local args = {[1] = "SyncTexture",[2] = {[1] = {["Part"] = part,["Face"] = side,["TextureType"] = "Decal",["Texture"] = "rbxassetid://".. asset}]} 
        _(args) 
    end
    
    function spam(id)
        local parts = {}  -- æ”¶é›†partsé¿å…é‡å¤
        for i,v in game.workspace:GetDescendants() do
            if v:IsA("BasePart") and not parts[v] then
                parts[v] = true
                spawn(function()
                    SetLocked(v,false)
                    SafeWait(0.1)  -- æ¯ä¸ªpartå»¶æ—¶0.1s
                    for _, side in pairs(Enum.NormalId:GetEnumItems()) do
                        SpawnDecal(v,side)
                        SafeWait(0.05)  -- æ¯ä¸ªé¢0.05s
                        AddDecal(v,id,side)
                    end
                end)
                SafeWait(0.2)  -- parté—´0.2s
            end
        end 
    end
    
    SafeWait(0.3)  -- å¯åŠ¨å»¶æ—¶
    spawn(function() spam(119360779637179) end)
    task.wait(3)  -- æ€»å†·å´3s
    Debounce.Decal = false
end)

-- å¤©ç©ºç›’ (ä¼˜åŒ–æ—‹è½¬ + å»¶æ—¶)
SkyboxBtn.MouseButton1Click:Connect(function()
    if Debounce.Skybox then return end
    Debounce.Skybox = true
    
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    SafeHD(";unfog")
    SafeWait(0.4)
    SafeHD(";fogcolor black")
    SafeWait(0.4)
    SafeHD(";time 0")
    
    -- Skyboxåˆ›å»ºé€»è¾‘ä¿æŒåŸæ ·ï¼Œä½†æ‰€æœ‰remoteåŠ å»¶æ—¶
    local player = game.Players.LocalPlayer
    local char = player.Character
    local tool
    for i,v in player:GetDescendants() do if v.Name == "SyncAPI" then tool = v.Parent end end
    for i,v in game.ReplicatedStorage:GetDescendants() do if v.Name == "SyncAPI" then tool = v.Parent end end
    local remote = tool.SyncAPI.ServerEndpoint
    function _(args) AddDelay(function() remote:InvokeServer(unpack(args)) end) end
    
    -- ... åŸSkyå‡½æ•°ä¿æŒï¼Œä½†MovePartåŠ å»¶æ—¶ ...
    function MovePart(part,cf) 
        local args = {[1] = "SyncMove",[2] = {[1] = {["Part"] = part,["CFrame"] = cf}}} 
        _(args) 
    end
    
    -- åŸSkyé€»è¾‘...
    local root = char.HumanoidRootPart
    local spawnPos = CFrame.new(root.Position) + Vector3.new(0,6,0)
    -- åˆ›å»ºskyPart (ç•¥ï¼Œä¿æŒåŸæ ·)
    
    task.wait(4)  -- Skyboxå†·å´4s
    Debounce.Skybox = false
end)

